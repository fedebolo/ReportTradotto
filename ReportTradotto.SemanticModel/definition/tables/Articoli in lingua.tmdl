	partition 'Articoli in lingua' = m
		mode: import
		queryGroup: Dimensioni
		source = ```
				let
				    Origine =
				        Sql.Database(
				            "WTCNB1023\SISTEMI",
				            "ESOLVER42",
				            [
				                Query = 
				"
				SELECT
				      qArtItaliano.K_CodArtVar
				    , qArtItaliano.CodArticolo
				        + CASE WHEN ISNULL(qArtItaliano.CodVariante, '') <> '' 
				            THEN '_' + qArtItaliano.CodVariante 
				            ELSE '' END AS [Articolo - Codice con Variante]
				
				    -- Inglese
				    , qArtItaliano.CodArticolo + '_' + ISNULL(qArtItaliano.CodVariante, '') 
				        + ' - ' + ISNULL(qArtInglese.[Articolo - Variante in lingua], qArtItaliano.[Articolo - Variante in italiano])
				        AS [Articolo e variante in inglese]
				
				    , CASE WHEN ISNULL(qArtItaliano.CodVariante, '') <> '' 
				        THEN qArtItaliano.CodVariante + ' - ' 
				             + ISNULL(qArtInglese.[Articolo - Variante in lingua], qArtItaliano.[Articolo - Variante in italiano])
				        ELSE '' END AS [Codice e descrizione variante in inglese]
				
				    , ISNULL(qArtInglese.[Articolo - Variante in lingua], qArtItaliano.[Articolo - Variante in italiano]) 
				        AS [Variante in inglese]
				
				    , qArtInglese.CodLingua AS [Codice Lingua]
				
				FROM (
				        SELECT
				              Art.DBGruppo
				            , Art.K_CodArtVar
				            , Art.CodArticolo
				            , Art.CodVariante
				            , CASE WHEN ISNULL(Art.CodVariante, '') <> ''
				                THEN Art.CodVariante + ' - ' + Art.DesArticoloVariante 
				                ELSE '' END AS [Articolo - Descrizione variante in italiano]
				            , Art.DesArticoloVariante AS [Articolo - Variante in italiano]
				        FROM dbo.ESBI_ARTVAR AS Art
				    ) qArtItaliano
				
				LEFT OUTER JOIN (
				        SELECT
				              Art.K_CodArtVar
				            , ISNULL(ArtLin.DesEstesa, Art.DesArticoloVariante) AS [Articolo - Variante in lingua]
				            , ArtLin.CodLingua AS CodLingua  
				
				        FROM ESBI_ARTVAR Art
				        LEFT OUTER JOIN ArtDatiInLingua ArtLin
				            ON Art.DBGruppo = ArtLin.DBGruppo
				            AND Art.CodArticolo = ArtLin.CodArt
				            AND Art.CodVariante = ArtLin.VarianteArt
				        LEFT OUTER JOIN CodiciLingua Lin
				            ON ArtLin.DBGruppo = Lin.DBGruppo
				            AND ArtLin.CodLingua = Lin.CodLingua
				        WHERE Lin.SiglaCodifInternaz = 'EN'
				    ) qArtInglese
				    ON qArtItaliano.K_CodArtVar = qArtInglese.K_CodArtVar
				
				WHERE qArtItaliano.DBGruppo IN ('XS')
				    AND (
				        qArtItaliano.CodArticolo NOT IN (
				            SELECT ContAlfanumerico
				            FROM SchedeTecValAmm
				            WHERE DBGruppo = 'XS'
				            AND IdEtichetta = 11
				            AND CodTipoSchedaTecnica = 'CODVEN'
				        )
				    );
				"
				                ,
				                CreateNavigationProperties = false
				            ]
				        )
				in
				    Origine
				```

