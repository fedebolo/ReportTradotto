	partition Commesse-a699aaa6-6224-40e2-b5d4-bc176a18cf5e = m
		mode: import
		queryGroup: Dimensioni
		source = ```
				let
				    StringaFiltroGruppoIN = if #"Filtro - Metodo filtro ditte" = "Presenti in elenco" then " AND DBGruppo IN (" & Gruppi & ")" else "",
				    StringaFiltroGruppoNOTIN = if #"Filtro - Metodo filtro ditte" = "Non presenti in elenco" then " AND DBGruppo NOT IN (" & Gruppi & ")" else "",
				    StringaTop0 = if #"Dettagli aggiuntivi: informazioni dei documenti" = "Disattiva" then "TOP (0)" else "",
				    Origine = Sql.Database(Server, Database, [Query= 
				    "
				        SELECT " & StringaTop0 & "
				            CodCommessa AS [Commessa - Codice]
				            ,DesCommessa AS [Commessa - Descrizione]
				            ,CodRaggrClasse1 AS [Commessa - Codice raggruppamento 1]
				            ,CodRaggrClasse2 AS [Commessa - Codice raggruppamento 2]
				            ,CodRaggrClasse3 AS [Commessa - Codice raggruppamento 3]
				            ,DesRaggrClasse1 AS [Commessa - Descrizione raggruppamento 1]
				            ,DesRaggrClasse2 AS [Commessa - Descrizione raggruppamento 2]
				            ,DesRaggrClasse3 AS [Commessa - Descrizione raggruppamento 3]
				            ,CodCliFor AS [Commessa - Codice cliente]
				            ,CodCentroImputazione AS [Commessa - Codice centro imputazione]
				            ,DesCentroImputazione AS [Commessa - Descrizione centro imputazione]
				            ,CodDivisione AS [Commessa - Codice divisione]
				            ,DesDivisione AS [Commessa - Descrizione divisione]
				            ,DataAperturaComm AS [Commessa - Data apertura]
				            ,DataChiusuraComm AS [Commessa - Data chiusura]
				            ,DataFinePrevistaComm AS [Commessa - Data fine prevista]
				            ,DataInizioComm AS [Commessa - Data inizio]
				            ,CodRespPrimoLiv AS [Commessa - Codice responsabile]
				            ,DesRespPrimoLiv AS [Commessa - Descrizione responsabile]
				            ,CodCdAAssegnazione AS [Commessa - Codice CdA assegnato]
				            ,DesCdAAssegnazione AS [Commessa - Descrizione CdA assegnato]
				            ,K_CodCommessa AS [K_CodCommessa]
				            ,Commessa AS [Commessa]
				            ,RaggrClasse1 AS [Commessa - Raggruppamento 1]
				            ,RaggrClasse2 AS [Commessa - Raggruppamento 2]
				            ,DesCliente AS [Commessa - Descrizione cliente]
				            ,Cliente AS [Commessa - Cliente]
				            ,CentroImputazione AS [Commessa - Centro imputazione]
				            ,Divisione AS [Commessa - Divisione]
				            ,RaggrClasse3 AS [Commessa - Raggruppamento 3]
				            ,RespPrimoLiv AS [Commessa - Responsabile]
				            ,CdAAssegnazione AS [Commessa - CdA assegnato]
				            ,DesStatoCommessa AS [Commessa - Stato]
				
				        FROM 
				            [dbo].[ESBI_COMM] WITH (NOLOCK)
				            
				        WHERE 
				            CodSottocommessa = ''
				    "
				        & StringaFiltroGruppoIN
				        & StringaFiltroGruppoNOTIN
				        & ";
				    "
				    ,CreateNavigationProperties=false])
				in 
				    Origine
				```

