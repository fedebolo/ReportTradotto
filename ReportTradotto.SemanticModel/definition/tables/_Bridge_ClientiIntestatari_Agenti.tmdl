	partition _Bridge_ClientiIntestatari_Agenti-37ccc252-d9d9-42a7-a9fc-6be03c9e6571 = m
		mode: import
		queryGroup: Dimensioni
		source = ```
				let
				    StringaFiltroGruppoIN = if #"Filtro - Metodo filtro ditte" = "Presenti in elenco" then " AND [ESBI_COND_COMM_AGE].DBGruppo IN (" & Gruppi & ")" else "",
				    StringaFiltroGruppoNOTIN = if #"Filtro - Metodo filtro ditte" = "Non presenti in elenco" then " AND [ESBI_COND_COMM_AGE].DBGruppo NOT IN (" & Gruppi & ")" else "",
				    Origine = Sql.Database(Server, Database, [Query= 
				    "
				        SELECT
				             [K_CodCliFor] AS [K_CodCli]
				            ,[ESBI_COND_COMM_AGE].[K_CodAgenteRuolo] AS [K_CodAgenteRuolo]
				            ,[TipoAgenteCondComm] AS [Condiz. comm. (Cliente) - Agente principale/secondario]
							,[K_CodAgente]
				            ,[ESBI_COND_COMM_AGE].DBGruppo as [Ditta]
				            
				        FROM 
				            [dbo].[ESBI_COND_COMM_AGE] WITH (NOLOCK) 
							INNER JOIN
							[dbo].[ESBI_AGE] WITH (NOLOCK) 
							ON [ESBI_COND_COMM_AGE].[K_CodAgenteRuolo]=[ESBI_AGE].[K_CodAgenteRuolo]
				            
				        WHERE 
				            ProgrCondComm = 0
				            AND TipoAgenteCondComm='Principale'"
				
				        & StringaFiltroGruppoIN
				        & StringaFiltroGruppoNOTIN
				        & ";
				    "
				    ,CreateNavigationProperties=false])
				in
				    Origine
				```

	annotation PBI_ResultType = Table

	annotation PBI_NavigationStepName = Navigazione

